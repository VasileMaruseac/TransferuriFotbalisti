<link rel="stylesheet" href="../../css/first.css" />
<body>
  <button id="home"><a href="/">Home</a></button>
  <div class="container">
    <h2>Lista tuturor echipelor</h2>
    <% for ( var i = 0; i< s.length; i++){ %> <% var linkToOneTeam =
    "/echipe/get/" + s[i].dataValues.idEchipa;%>
    <div class="row">
      <div class="col"></div>

      <div class="col-6">
        <a href="<%- linkToOneTeam%>"><h2><%- s[i].dataValues.nume %></h2></a>
        <% const deleteString = "/echipe/delete/" +s[i].dataValues.idEchipa; %>
        <a href="<%- deleteString%>" data-method="delete">Sterge</a>
      </div>
      <div class="col"></div>
    </div>
    <% } %>
    <button><a href="/echipe/add">Creeaza o echipa noua</a></button>
  </div>
</body>
